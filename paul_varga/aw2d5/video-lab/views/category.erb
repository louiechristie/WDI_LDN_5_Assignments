<% @videos.each do |videos| %>

<div id="genre">

  <h3>Title: <%= videos["title"] %></h3>
  <p>Description: <%= videos["description"] %></p>
  <p><iframe width="420" height="315" src="//www.youtube.com/embed/<%= videos['video_id'] %>" frameborder="0" allowfullscreen></iframe></p>

</div>

<% end %>